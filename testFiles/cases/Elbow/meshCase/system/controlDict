FoamFile
{
    version   2.0;
    format    ascii;
    class     dictionary;
    location  "system";
    object    mesh;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application         cfMesh;

deltaT              1;

endTime             1;

purgeWrite          1;

runTimeModifiable   yes;

startFrom           latestTime;

startTime           0;

stopAt              endTime;

writeControl        timeStep;

writeFormat         ascii;

writeInterval       1;

writePrecision      15;

