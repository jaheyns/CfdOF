{
    transportModel  Newtonian;
    nu              [0 2 -1 0 0 0 0] %(fluidProperties/%(0%)/KinematicViscosity%);
    rho             [1 -3 0 0 0 0 0] %(fluidProperties/%(0%)/Density%);
}

%}
sigma           [1 0 -2 0 0 0 0] 0.0;
%:multiphaseInterFoam
