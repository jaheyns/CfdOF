    %(0%)
    {
        type        calculated;
        value       $internalField;
    }
%}
%:empty
%[0/_boundary_empty%]
%:wall
%{%(boundaries/%(0%)/BoundarySubType%)
%:fixedWall translatingWall partialSlipWall
    %(0%)
    {
%{%(physics/TurbulenceModel%)
