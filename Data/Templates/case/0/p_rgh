        p0          uniform %(boundaries/%(0%)/Pressure%);
%}
        value       $internalField;
    }
%:default
%[0/_boundary_fixedFluxPressure%]
%}
%:open
    %(0%)
    {
        type        totalPressure;
        p0          uniform %(boundaries/%(0%)/Pressure%);
        value       $internalField;
    }
